    "features_train_ordinal[numeric] = scaler.transform(features_train_ordinal[numeric])\n",
    "features_valid_ordinal[numeric] = scaler.transform(features_valid_ordinal[numeric])\n",
    "\n",
    "features_train_ordinal"
