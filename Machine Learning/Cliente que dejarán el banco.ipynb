    "# Estandrizar de datos\n",
    "numeric = [\"CreditScore\", \"Age\", \"Tenure\", \"Balance\", \"NumOfProducts\", \"HasCrCard\", \"IsActiveMember\", \"EstimatedSalary\"]\n",
    "\n",
    "scaler = StandardScaler()\n",
