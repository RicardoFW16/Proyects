   "source": [
    "# Desarrollar modelo de árbol de decisiones\n",
    "model = DecisionTreeClassifier(random_state=12345)\n",
    "model.fit(features_train_ordinal, target_train_ordinal)\n",
    "\n",
    "predicted_valid = model.predict(features_valid_ordinal)\n",
    "\n",
    "accuracy_valid = accuracy_score(target_valid_ordinal, predicted_valid)\n",
    "\n",
    "print(accuracy_valid)"
